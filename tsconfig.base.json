{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "moduleResolution": "node",
    "baseUrl": "./",
    "outDir": "./dist",
    "importHelpers": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "paths": {
      "@personality-questionnaire/data": [
        "libs/data/src/index.ts"
      ],
      "@personality-questionnaire/interfaces": [
        "libs/interfaces/src/index.ts"
      ]
    }
  },
  "include": [
    "src/**/*"
  ]
}
